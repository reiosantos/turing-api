{
	"name": "turing-api",
	"version": "1.0.0",
	"author": "reio santos",
	"main": "src/index.js",
	"description": "e-commerce application",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/reiosantos/turing-api.git"
	},
	"homepage": "https://github.com/reiosantos/turing-api#readme",
	"bugs": {
		"url": "https://github.com/reiosantos/turing-api/issues"
	},
	"keywords": [
		"ecommerce",
		"nodejs",
		"express",
		"caching"
	],
	"scripts": {
		"serve": "nodemon --exec babel-node lib/src/index.js",
		"build": "rimraf lib/ && NODE_ENV=production babel ./ --out-dir lib/ --ignore ./node_modules",
		"start": "NODE_ENV=production npm run build && npm run serve",
		"start:dev": "NODE_ENV=development nodemon --exec babel-node src/index.js",
		"lint": "node_modules/.bin/eslint ./",
		"test": "NODE_ENV=test mocha --require @babel/register --require dotenv/config --exit",
		"test:coverage": "NODE_ENV=test nyc mocha --require @babel/register --require dotenv/config --exit",
		"db:seed": "node_modules/.bin/sequelize db:seed:all",
		"db:migrate": "node_modules/.bin/sequelize db:migrate",
		"db:rollback": "node_modules/.bin/sequelize db:migrate:undo:all",
		"db:rollmigrate": "npm run db:rollback && npm run db:migrate && npm run db:seed"
	},
	"dependencies": {
		"@babel/polyfill": "^7.6.0",
		"@babel/runtime": "^7.2.0",
		"@sendgrid/mail": "^6.3.1",
		"bcrypt": "^5.0.0",
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"cookie-parser": "~1.4.4",
		"cors": "^2.8.5",
		"coveralls": "^3.0.2",
		"debug": "^4.1.1",
		"dotenv": "^6.2.0",
		"express": "~4.16.1",
		"express-jwt": "^5.3.1",
		"express-validator": "^5.3.1",
		"express-winston": "^3.3.0",
		"fancy-log": "^1.3.3",
		"helmet": "^3.16.0",
		"http-errors": "~1.6.3",
		"istanbul": "^0.4.5",
		"jsonwebtoken": "^8.4.0",
		"mailgun-js": "^0.22.0",
		"moment": "^2.24.0",
		"morgan": "~1.9.1",
		"mysql": "^2.16.0",
		"mysql2": "^1.6.4",
		"node-cache": "^4.2.1",
		"node-sass-middleware": "0.11.0",
		"nodemon": "^1.18.9",
		"passport": "^0.4.0",
		"passport-local": "latest",
		"sequelize": "^5.8.6",
		"sequelize-cli": "^5.4.0",
		"stripe": "^6.28.0",
		"swagger-jsdoc": "^3.2.9",
		"swagger-ui-express": "^4.0.2",
		"uniqid": "^5.0.3",
		"validator": "^10.9.0",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.2.2",
		"@babel/node": "^7.2.2",
		"@babel/plugin-proposal-class-properties": "^7.2.3",
		"@babel/plugin-transform-runtime": "^7.2.0",
		"@babel/preset-env": "^7.2.3",
		"@babel/register": "^7.0.0",
		"babel-eslint": "^10.0.1",
		"babel-plugin-istanbul": "^5.1.0",
		"chai": "^4.2.0",
		"chai-http": "^4.2.0",
		"eslint": "^5.10.0",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-config-prettier": "^3.3.0",
		"eslint-plugin-import": "^2.14.0",
		"mocha": "^5.2.0",
		"nyc": "^14.1.1",
		"prettier": "^1.15.3",
		"rimraf": "^2.6.2",
		"sinon": "^7.2.2"
	}
}
